{
    "name": "jpry/advent-of-code",
    "description": "Solutions to adventofcode.com.",
    "authors": [
        {
            "name": "Jeremy Pry",
            "email": "jeremy.pry@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.2",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/dotenv": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "JPry\\AdventOfCode\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
